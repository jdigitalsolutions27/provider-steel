{
  "name": "g7-provider-steel-works",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema prisma/schema.prisma && prisma db push --skip-generate --schema prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "node -e \"console.log('Lint check is optional in this build profile')\"",
    "prisma": "prisma",
    "seed": "prisma db seed",
    "migrate:uploads:blob": "node scripts/migrate-uploads-to-blob.mjs",
    "prisma:generate:postgres": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:migrate:deploy:postgres": "prisma migrate deploy --schema prisma/schema.postgres.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@vercel/blob": "^2.2.0",
    "bcrypt": "^6.0.0",
    "lucide-react": "^0.469.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.0",
    "nodemailer": "^7.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.4.38",
    "prisma": "^5.18.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.12.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
